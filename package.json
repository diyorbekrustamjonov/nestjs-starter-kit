{
  "name": "nestsjs-starter-kit",
  "version": "1.0.1",
  "description": "NestJS Starter Kit",
  "main": "build/index.js",
  "scripts": {
    "start": "nest start index.js",
    "start:build": "del-cli -rf build && tsc && tsc-alias",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node build/index.js",
    "prestart:prod": "npm run build",
    "seed": "node build/core/shared/seeders/seeder.js",
    "seed:refresh": "node build/core/shared/seeders/seeder.js --refresh",
    "lint": "npm run lint:eslint && npm run lint:tslint && npm run lint:prettier",
    "lint:eslint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:prettier": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "coverage": "nyc npm run test"
  },
  "engines": {
    "node": "16.19.1",
    "npm": "8.19.3"
  },
  "keywords": [
    "nestjs",
    "starter",
    "kit"
  ],
  "author": {
    "name": "Diyorbek Rustamjonov",
    "email": "drustamjonov0919@gmail.com",
    "url": "https://diyorbek.ru"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/test/**/*",
      "**/*.js",
      "**/config.ts",
      "./coverage"
    ],
    "excludeNodeModules": true,
    "reporter": [
      "text",
      "lcov"
    ],
    "all": true
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/mongoose": "^9.2.1",
    "@nestjs/passport": "^9.0.1",
    "@nestjs/platform-express": "^9.3.12",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/throttler": "^4.0.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "helmet": "^6.0.1",
    "minio": "^7.0.33",
    "mongoose": "^6.9.1",
    "mongoose-paginate-v2": "^1.7.1",
    "multer": "^1.4.5-lts.1",
    "nestjs-minio": "^2.3.2",
    "nestjs-seeder": "^0.3.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "sharp": "^0.32.0",
    "tslib": "^2.3.1",
    "tslog": "3.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/minio": "^7.0.18",
    "@types/multer": "^1.4.7",
    "@types/sharp": "^0.31.1",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/node": "18.11.18",
    "@types/source-map-support": "^0.5.4",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mongoose-slug-updater": "^3.3.0",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.6",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.9.5"
  },
  "license": "MIT"
}
